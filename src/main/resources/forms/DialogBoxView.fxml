<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane fx:id="DIALOG_PANE" prefHeight="0.0" prefWidth="0.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <bottom>
      <AnchorPane BorderPane.alignment="CENTER">
         <children>
            <HBox fx:id="DIALOG_BUTTON_BOX" spacing="10.0" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0">
               <children>
                  <Button fx:id="DIALOG_BUTTON_OK" mnemonicParsing="false" prefHeight="25.0" prefWidth="65.0" text="Ok" />
                  <Button fx:id="DIALOG_BUTTON_NO" mnemonicParsing="false" prefHeight="25.0" prefWidth="65.0" text="No" />
                  <Button fx:id="DIALOG_BUTTON_ACTION" mnemonicParsing="false" prefHeight="25.0" prefWidth="65.0" text="Action" />
               </children>
            </HBox>
         </children>
      </AnchorPane>
   </bottom>
</BorderPane>
