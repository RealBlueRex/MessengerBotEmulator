<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="anpActivityBar" minWidth="40.0" prefWidth="40.0" styleClass="activity-bar" stylesheets="@../styles/ActivityBarPart.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox fx:id="voxActivityBar" prefWidth="40.0">
         <children>
            <ToggleButton fx:id="tgnExplorerTab" mnemonicParsing="false" prefHeight="50.0" prefWidth="40.0" selected="true" styleClass="explorer-tab" />
            <ToggleButton fx:id="tgnDebugTab" mnemonicParsing="false" prefHeight="50.0" prefWidth="40.0" styleClass="debug-tab" />
         </children>
      </VBox>
   </children>
</AnchorPane>
