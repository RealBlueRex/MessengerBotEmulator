<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane fx:id="DIALOG_PANE" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <bottom>
      <AnchorPane BorderPane.alignment="CENTER">
         <children>
            <HBox fx:id="DIALOG_BUTTON_BOX" spacing="10.0" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
               <children>
                  <Button fx:id="DIALOG_BUTTON_OK" mnemonicParsing="false" prefHeight="27.0" prefWidth="70.0" styleClass="button-ok" text="Ok" />
                  <Button fx:id="DIALOG_BUTTON_NO" mnemonicParsing="false" prefHeight="27.0" prefWidth="70.0" styleClass="button-no" text="No" />
               </children>
            </HBox>
         </children>
      </AnchorPane>
   </bottom>
   <left>
      <AnchorPane BorderPane.alignment="CENTER">
         <children>
            <ImageView fx:id="DIALOG_ICON" fitHeight="45.0" fitWidth="45.0" layoutX="10.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="15.0" />
         </children>
      </AnchorPane>
   </left>
   <stylesheets>
      <URL value="@../skins/ButtonSkin.css" />
      <URL value="@../skins/CheckBoxSkin.css" />
      <URL value="@../skins/TextFieldSkin.css" />
      <URL value="@../styles/DialogBoxView.css" />
   </stylesheets>
</BorderPane>
